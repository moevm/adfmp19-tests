<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="ru.moevm.examapp">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity
                android:name=".MainActivity"
                android:label="@string/app_name"
                android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".PractiseActivity"
                  android:label="@string/practice"
                  android:parentActivityName=".MainActivity"/>
        <activity android:name=".TestSessionActivity"
                  android:label="@string/test_session"
                  android:parentActivityName=".MainActivity"/>
        <activity android:name=".FlashCardActivity"
                  android:label="@string/flashcards"
                  android:parentActivityName=".MainActivity"></activity>
    </application>
</manifest>